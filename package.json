{
  "name": "package-compliant",
  "version": "1.1.0",
  "description": "Validate the `support` property in the `package.json`",
  "main": "lib/index.js",
  "bin": {
    "package-compliant": "lib/cli.js"
  },
  "support": {
    "target": "NONE",
    "response": "BEST-EFFORT",
    "backing": "HOBBY"
  },
  "scripts": {
    "test": "standard && tap -J --coverage test/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eomm/package-compliant.git"
  },
  "keywords": [
    "package",
    "package-json",
    "validation",
    "support"
  ],
  "author": "Manuel Spigolon <behemoth89@gmail.com> (https://github.com/Eomm)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eomm/package-compliant/issues"
  },
  "homepage": "https://github.com/Eomm/package-compliant#readme",
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "ajv": "^6.9.2",
    "chalk": "^2.4.2",
    "commist": "^1.1.0",
    "yargs-parser": "^13.0.0"
  },
  "devDependencies": {
    "standard": "^12.0.1",
    "tap": "^12.6.3"
  }
}